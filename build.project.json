{
    "name": "RobloxProjectTemplate",
    "emitLegacyScripts": false,
    "globIgnorePaths": [
        "dist/__tests__/**",
        "test_runner/run_tests.luau",
        "lib/**"
    ],
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Packages": {
                "$path": "Packages"
            },
            "Client": {
                "$path": "dist/Client"
            },
            "Common": {
                "$path": "dist/Common"
            }
        },
        "ServerScriptService": {
            "Server": {
                "$path": "dist/Server"
            }
        }
    }
}