{
    "name": "Roblox Project Build",
    "emitLegacyScripts": false,
    "globIgnorePaths": [
        "dist/__tests__/**",
        "test_runner/run_tests.luau",
        "lib/**"
    ],
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Packages": {
                "$path": "Packages"
            },
            "Common": {
                "$path": "dist/Common"
            },
            "Assets": {
                "$path": "assets"
            }
        },
        "StarterPlayer": {
            "StarterPlayerScripts": {
                "Client": {
                    "$path": "dist/Client"
                }
            }
        },
        "Workspace": {
            "$path": "map/Workspace.rbxm"
        },
        "Lighting": {
            "$path": "map/Lighting.rbxm"
        },
        "ServerScriptService": {
            "Server": {
                "$path": "dist/Server"
            }
        }
    }
}