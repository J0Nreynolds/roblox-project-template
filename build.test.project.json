{
    "name": "Roblox Project Test Build",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Packages": {
                "$path": "Packages"
            },
            "DevPackages": {
                "$path": "DevPackages"
            },
            "lib": {
                "$path": "lib"
            },
            "Project": {
                "$path": "dist"
            }
        },
        "ServerScriptService": {
            "TestRunner": {
                "$path": "test_runner/run-tests.luau"
            },
            "$properties": {
                "LoadStringEnabled": true
            }
        }
    }
}