{
    "name": "RobloxProjectTemplate",
    "emitLegacyScripts": false,
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "Packages": {
                "$path": "Packages"
            },
            "DevPackages": {
                "$path": "DevPackages"
            },
            "lib": {
                "$path": "lib"
            },
            "Client": {
                "$path": "src/Client"
            },
            "Common": {
                "$path": "src/Common"
            },
            "Tests": {
                "$path": "src/__tests__"
            }
        },
        "ServerScriptService": {
            "Server": {
                "$path": "src/Server"
            },
            "TestRunner": {
                "$path": "run-tests.luau"
            }
        }
    }
}